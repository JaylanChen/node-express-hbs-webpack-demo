{
  "name": "express-hbs-webpack-demo",
  "version": "0.1.0",
  "description": "node express hbs(handlebars) webpack demo for server side renderer",
  "main": "server.js",
  "scripts": {
    "start": "cross-env NODE_ENV=local supervisor -w routes,utils,build,server.js,server.common.js -e js -i client -- --inspect=9396 --max-old-space-size=3072 ./server.js",
    "build": "webpack --config ./build",
    "build:production": "cross-env NODE_ENV=production --max-old-space-size=2048 webpack --config ./build",
    "server": "node ./server.js",
    "production": "npm run build:production && cross-env NODE_ENV=production node ./server.js",
    "test": "cross-env NODE_ENV=test node ./test.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/JaylanChen/node-express-hbs-webpack-demo.git"
  },
  "keywords": [
    "node",
    "express",
    "hbs",
    "handlebars",
    "webpack"
  ],
  "author": "JaylanChen",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/JaylanChen/node-express-hbs-webpack-demo/issues"
  },
  "homepage": "https://github.com/JaylanChen/node-express-hbs-webpack-demo#readme",
  "dependencies": {
    "axios": "^0.21.0",
    "body-parser": "^1.19.0",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.5",
    "core-js": "^3.7.0",
    "express": "^4.17.1",
    "express-async-errors": "^3.1.1",
    "express-handlebars": "^5.2.0",
    "express-winston": "^4.0.5",
    "handlebars-helpers": "^0.10.0",
    "jquery": "^3.5.1",
    "regenerator-runtime": "^0.13.7",
    "sass": "^1.29.0",
    "sass-loader": "^10.1.0",
    "serve-favicon": "^2.5.0",
    "uuid": "^8.3.1",
    "winston": "^3.3.3",
    "winston-daily-rotate-file": "^4.5.0",
    "winston-transport": "^4.4.0"
  },
  "devDependencies": {
    "@babel/core": "^7.12.3",
    "@babel/plugin-proposal-class-properties": "^7.12.1",
    "@babel/plugin-proposal-export-default-from": "^7.12.1",
    "@babel/plugin-proposal-export-namespace-from": "^7.12.1",
    "@babel/preset-env": "^7.12.1",
    "babel-loader": "^8.2.1",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^6.3.1",
    "cross-env": "^7.0.2",
    "css-loader": "^5.0.1",
    "express-template-reload": "^1.0.8",
    "handlebars-loader": "^1.7.1",
    "html-webpack-custominject-plugin": "^2.0.0",
    "html-webpack-harddisk-plugin": "^1.0.2",
    "html-webpack-plugin": "^4.5.0",
    "mini-css-extract-plugin": "^1.3.1",
    "optimize-css-assets-webpack-plugin": "^5.0.4",
    "raw-loader": "^4.0.2",
    "reload": "^3.1.1",
    "style-loader": "^2.0.0",
    "supervisor": "^0.12.0",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "url-loader": "^4.1.1",
    "webpack": "^5.5.0",
    "webpack-cli": "^4.2.0",
    "webpack-dev-middleware": "^4.0.2",
    "webpack-hot-middleware": "^2.25.0",
    "webpack-merge": "^5.4.0"
  }
}
