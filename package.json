{
  "name": "express-hbs-webpack-demo",
  "version": "0.1.0",
  "description": "node express hbs(handlebars) webpack demo for server side renderer",
  "main": "server.js",
  "scripts": {
    "start": "cross-env NODE_ENV=local supervisor -w routes,utils,build,server.js,server.common.js -e js -i client -- --inspect=9396 --max-old-space-size=3072 ./server.js",
    "build": "webpack --config ./build",
    "build:production": "cross-env NODE_ENV=production --max-old-space-size=2048 webpack --config ./build",
    "server": "node ./server.js",
    "production": "npm run build:production && cross-env NODE_ENV=production node ./server.js",
    "test": "cross-env NODE_ENV=test node ./test.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/JaylanChen/node-express-hbs-webpack-demo.git"
  },
  "keywords": [
    "node",
    "express",
    "hbs",
    "handlebars",
    "webpack"
  ],
  "author": "JaylanChen",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/JaylanChen/node-express-hbs-webpack-demo/issues"
  },
  "homepage": "https://github.com/JaylanChen/node-express-hbs-webpack-demo#readme",
  "dependencies": {
    "@babel/core": "^7.4.3",
    "@babel/plugin-proposal-class-properties": "^7.4.0",
    "@babel/plugin-transform-runtime": "^7.4.3",
    "@babel/preset-env": "^7.4.3",
    "@babel/runtime-corejs3": "^7.4.3",
    "autodll-webpack-plugin": "^0.4.2",
    "axios": "^0.18.0",
    "babel-loader": "^8.0.5",
    "body-parser": "^1.18.3",
    "clean-webpack-plugin": "^2.0.1",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.4",
    "copy-webpack-plugin": "^5.0.2",
    "core-js": "^3.0.1",
    "cross-env": "^5.2.0",
    "css-loader": "^2.1.1",
    "express": "^4.16.4",
    "express-async-errors": "^3.1.1",
    "express-handlebars": "^3.0.2",
    "express-winston": "^3.1.0",
    "handlebars-helpers": "^0.10.0",
    "handlebars-loader": "^1.7.1",
    "html-webpack-custominject-plugin": "^1.0.1",
    "html-webpack-plugin": "^3.2.0",
    "jquery": "^3.4.0",
    "mini-css-extract-plugin": "^0.6.0",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "raw-loader": "^2.0.0",
    "serve-favicon": "^2.5.0",
    "uglifyjs-webpack-plugin": "^2.1.2",
    "url-loader": "^1.1.2",
    "webpack": "^4.30.0",
    "webpack-cli": "^3.3.0",
    "webpack-merge": "^4.2.1",
    "winston": "^3.2.1",
    "winston-daily-rotate-file": "^3.8.0",
    "winston-transport": "^4.3.0"
  },
  "devDependencies": {
    "express-handlebars-memory-fs": "^1.0.0",
    "express-template-reload": "^1.0.8",
    "reload": "^3.0.0",
    "style-loader": "^0.23.1",
    "supervisor": "^0.12.0",
    "webpack-dev-middleware": "^3.6.2",
    "webpack-hot-middleware": "^2.24.3"
  }
}
